  cout << "Available commands: ADD, SEARCH, DELETE, QUIT." << endl;
  char input[10];
  
  while (true) {
    cout << "Awaiting new command "
    cin >> input;

    if (input[0] == 'A' &&
	input[1] == 'D' &&
	input[2] == 'D') {
      //addFunction();
      cout << "1" << endl;
    }

    if (input[0] == 'S' &&
	input[1] == 'E' &&
	input[2] == 'A' &&
	input[3] == 'R' &&
	input[4] == 'C' &&
	input[5] == 'H') {
      //searchFunction();
      cout << "2" << endl;
    }

    if (input[0] == 'D' &&
	input[1] == 'E' &&
	input[2] == 'L' &&
	input[3] == 'E' &&
	input[4] == 'T' &&
	input[5] == 'E') {
      //deleteFunction();
      cout << "3" << endl;
    }
    
  }
}
